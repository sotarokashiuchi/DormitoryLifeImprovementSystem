{% extends "./base.html" %}

{% block body %}
  <a href="/ryosyoku_order/control/input">入力</a>
  <h1>管理者画面 : 集計</h1>

  {% for date, time, set, food_name, people_count, not_write in reservation_list %}
      <p>{{date}}({{time}})</p>
      <p>{{set[0]}}:{{food_name[0]}}{{people_count[0]}}</p>
      <p>{{set[1]}}:{{food_name[1]}}{{people_count[1]}}</p>
      <p>未入力者:{{not_write}}</p>
  {% endfor %}
{% endblock %}