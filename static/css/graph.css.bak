/* 棒グラフの枠組み */
.graph-main {
  display: flex;
  align-items: flex-end;
  height: 200px;
  max-width: 303px;
  margin: 0 auto;
  border-left: 3px solid rgb(218,220,224);
}

/* 棒 */
.graph-main .bar {
  width: 19px;
  margin: 0 0.5px;
  
  /* background: rgb(26,115,232); */
  /* 丸角 */
  border-radius: 4px 4px 0 0;
}

.graph-axis-padding {
  display: flex;
  width: 300px;
  height: 10px;
  margin: 0 auto;
}

.graph-axis-padding .padding {
  width: 60px;
  border-left: 3px solid rgb(218,220,224);
  border-top: 3px solid rgb(218,220,224);
  /* border-style: solid none none none; */
}

.graph-axis {
  display: flex;
  width: 300px;
  height: 50px;

  margin: 0 auto;
  /* width: 50px; */
  /* background-color: grey; */
}

.graph-axis .axis{
  width: 60px;
  text-align: center;
}

.graph-axis .axis18{
  margin-left: 30px;
}

.graph-axis .axis21{
  margin-right: 30px;
}